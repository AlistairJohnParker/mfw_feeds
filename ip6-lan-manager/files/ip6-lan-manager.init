#!/bin/sh /etc/rc.common
# Copyright (C) 2006-2011 OpenWrt.org

START=99
STOP=99

USE_PROCD=1

boot() {
    start
}

start_service() {
    procd_open_instance ip6-lan-manager
    procd_set_param command /usr/bin/ip6-lan-manager
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_close_instance
}

